{
    "name": "repo",
    "version": "1.0.0",
    "description": "test ",
    "main": "index.html",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "lite-server",
        "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
        "scss": "node-sass -o css/ css/",
        "watch:scss": "onchange 'css/*.scss' -- npm run scss ",
        "clean": "rimraf dist",
        "imagemin": "imagemin images/* --out-dir dist/images",
        "usemin": "usemin about.html -d dist --htmlmin -o dist/about.html && usemin price.html -d dist --htmlmin -o dist/price.html && usemin contact.html -d dist --htmlmin -o dist/contact.html && usemin index.html -d dist --htmlmin -o dist/index.html",
        "build": "npm run clean && npm run scss && npm run && npm run imagemin && npm run usemin"
    },
    "repository": {
        "type": "git",
        "url": "git+https://yhizuz@bitbucket.org/yhizuz/repo.git"
    },
    "author": "jesus muriel",
    "license": "ISC",
    "homepage": "https://bitbucket.org/yhizuz/repo#readme",
    "devDependencies": {
        "copyfiles": "^2.4.0",
        "cssmin": "^0.4.3",
        "htmlmin": "0.0.7",
        "lite-server": "^2.6.1",
        "node-sass": "^5.0.0",
        "onchange": "^7.1.0",
        "rimraf": "^3.0.2",
        "uglifyjs": "^2.4.11",
        "usemin-cli": "^0.6.0"
    },
    "dependencies": {
        "bootstrap": "^4.5.3",
        "concurrently": "^5.3.0",
        "jquery": "^3.5.1",
        "open-iconic": "^1.1.1",
        "popper.js": "^1.16.1",
        "sass": "^1.29.0",
        "usemin": "^0.6.0"
    }
}